{
  "name": "switchy-nemo",
  "description": "Switchy Nemo is a browser extension that allows you to switch between different proxy servers easily.",
  "version": "0.0.2",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "compile": "tsc --noEmit",
    "postinstall": "wxt prepare",
    "lint": "eslint . --fix --max-warnings=0",
    "format": "prettier --write --list-different \"./**/*.{js,jsx,ts,tsx,css,md,json,yaml}\" "
  },
  "dependencies": {
    "async-validator": "^4.2.5",
    "clsx": "^2.1.1",
    "i18next": "^25.4.2",
    "mobx": "^6.13.7",
    "mobx-react-lite": "^4.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-i18next": "^15.7.3",
    "react-router": "^7.5.3",
    "react-transition-group": "4.4.5",
    "ress": "^5.0.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.27.1",
    "@eslint/js": "^9.25.0",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.2",
    "@types/react-transition-group": "4.4.5",
    "@wxt-dev/module-react": "^1.1.3",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "postcss": "^8.5.3",
    "postcss-nested": "^7.0.2",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite-plugin-svgr": "^4.3.0",
    "wxt": "^0.20.0"
  }
}
